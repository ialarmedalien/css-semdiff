{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "outDir": "dist",
        "rootDir": "src",
        "lib": ["es2015"],
        "noEmitOnError": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules"
    ]
}
